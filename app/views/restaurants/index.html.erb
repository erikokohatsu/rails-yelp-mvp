<div class="container">

<h1>All restaurants</h1>

<ul>
  <% @restaurants.each do |restaurant| %>
  <li><%= link_to restaurant.name, restaurant_path(restaurant[:id]) %></li>
  <p><%= restaurant.category %></p>
  <% end %>
</ul>

<p><%= link_to "Add new restaurant", new_restaurant_path %></p>
</div>
